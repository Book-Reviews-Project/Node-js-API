{
  "name": "Node-js-API",
  "engines": {
    "node": ">=8.0.0"
  },
  "version": "1.0.0",
  "description": "Node.js Back-End for Book Reviews",
  "main": "index.js",
  "scripts": {
    "start:dev": "NODE_ENV=development nodemon -r dotenv/config ./bin/www",
    "start:prod": "NODE_ENV=production nodemon -r dotenv/config ./bin/www",
    "sequelize:init": "sudo sequelize init",
    "sequelize:migrate": "NODE_ENV=development sudo sequelize db:migrate",
    "sequelize:migrate:prod": "NODE_ENV=production sudo sequelize db:migrate",
    "sequelize:migrate:undo:all": "NODE_ENV=development sudo sequelize db:migrate:undo:all",
    "sequelize:migrate:undo:all:prod": "NODE_ENV=production sudo sequelize db:migrate:undo:all",
    "test": "nyc --reporter=html --reporter=text mocha --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Book-Reviews-Project/Node-js-API.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Book-Reviews-Project/Node-js-API/issues"
  },
  "homepage": "https://github.com/Book-Reviews-Project/Node-js-API#readme",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.0",
    "mysql2": "^1.6.1",
    "sequelize": "^4.38.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.3.0",
    "eslint-plugin-mocha": "^5.1.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^5.2.0",
    "node-mocks-http": "^1.7.0",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "sequelize-cli": "^4.0.0",
    "sequelize-test-helpers": "^1.0.3",
    "sinon": "^6.1.4",
    "sinon-chai": "^3.2.0",
    "supertest": "^3.1.0"
  }
}
